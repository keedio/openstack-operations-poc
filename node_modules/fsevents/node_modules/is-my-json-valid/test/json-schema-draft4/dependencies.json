[
	{
		"description": "dependencies",
		"schema": {
			"dependencies": {
				"bar": [
					"foo"
				]
			}
		},
		"tests": {
			"description": "ignores non-objects",
			"data": "foo",
			"valid": true
		}
	},
	{
		"description": "multiple dependencies",
		"schema": {
			"dependencies": {
				"quux": [
					"foo",
					"bar"
				]
			}
		},
		"tests": {
			"description": "missing both dependencies",
			"data": {
				"quux": 1
			},
			"valid": false
		}
	},
	{
		"description": "multiple dependencies subschema",
		"schema": {
			"dependencies": {
				"bar": {
					"properties": {
						"foo": {
							"type": "integer"
						},
						"bar": {
							"type": "integer"
						}
					}
				}
			}
		},
		"tests": {
			"description": "wrong type both",
			"data": {
				"foo": "quux",
				"bar": "quux"
			},
			"valid": false
		}
	}
]
